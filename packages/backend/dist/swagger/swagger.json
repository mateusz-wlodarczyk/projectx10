    "paths": {
        "/": {
            "get": {
                "operationId": "GetWelcome",
                "responses": {
                    "200": {
                        "description": "A welcome message",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "description": "Welcome endpoint",
                "tags": [
                    "Base"
                ],
                "security": [],
                "parameters": []
            }
        },
        "/boat": {
            "get": {
                "operationId": "GetBoatData",
                "responses": {
                    "200": {
                        "description": "Price history data for the boat",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "boat": {
                                            "$ref": "#/components/schemas/WeeklyPriceHistory"
                                        },
                                        "slug": {
                                            "type": "string"
                                        }
                                    },
                                    "required": [
                                        "boat",
                                        "slug"
                                    ],
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "description": "Get boat data for a specific slug, week, and year",
                "tags": [
                    "Boats"
                ],
                "security": [],
                "parameters": [
                    {
                        "description": "The slug of the boat",
                        "in": "query",
                        "name": "slug",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "The week number",
                        "in": "query",
                        "name": "week",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "The year",
                        "in": "query",
                        "name": "year",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ]
            }
        }
    },
    "servers": [
        {
            "url": "/"
        }
    ]
